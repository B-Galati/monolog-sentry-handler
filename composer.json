{
    "name": "homeapp/monolog-sentry-handler",
    "description": "Sentry handler for php SDK",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Benoit Galati",
            "email": "benoit.galati@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Homeapp\\MonologSentryHandler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Homeapp\\MonologSentryHandler\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "monolog/monolog": "^2.2",
        "sentry/sentry": "^3.2"
    },
    "require-dev": {
        "coduo/php-matcher": "^3.2.2",
        "friendsofphp/php-cs-fixer": "^2.15",
        "jangregor/phpstan-prophecy": "0.8.0",
        "phpstan/phpstan": "0.12.25",
        "phpstan/phpstan-phpunit": "0.12.8",
        "phpunit/phpunit": "^7.5||^8.3",
        "symfony/phpunit-bridge": "^4.3",
        "symfony/var-dumper": ">4.3"
    }
}
